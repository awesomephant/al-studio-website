{
  "name": "al-website",
  "private": true,
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "build:webpack": "webpack --mode production",
    "build:eleventy": "npx @11ty/eleventy",
    "build:sass": "sass ./sass/style.scss ./_site/css/style.css --no-source-map --style=compressed",
    "watch:webpack": "webpack --watch --mode development",
    "watch:eleventy": "npx @11ty/eleventy --serve --quiet",
    "watch:sass": "sass ./sass/style.scss ./_site/css/style.css --watch",
    "build": "run-s 'build:*'",
    "start": "run-p 'watch:*'"
  },
  "author": "Max Kohler",
  "license": "ISC",
  "dependencies": {
    "@11ty/eleventy": "^2.0.0",
    "@11ty/eleventy-plugin-rss": "^1.1.2",
    "jsdom": "^19.0.0",
    "netlify-cms-app": "^2.15.72",
    "npm-run-all": "^4.1.5",
    "sass": "^1.50.1",
    "webpack": "^5.72.0",
    "webpack-cli": "^5.0.1"
  },
  "devDependencies": {
    "imports-loader": "^3.1.1"
  },
  "packageManager": "yarn@3.4.1"
}
